    <!-- http://ontology.mayo.edu/ontologies/clinicalsituationontology/5e1a5de8-1b49-3f25-b5f6-3621174f98d7 -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontologies/clinicalsituationontology/5e1a5de8-1b49-3f25-b5f6-3621174f98d7">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://snomed.info/id/413350009"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://snomed.info/id/408729009"/>
                        <owl:someValuesFrom rdf:resource="http://snomed.info/id/36692007"/>
                    </owl:Restriction>
