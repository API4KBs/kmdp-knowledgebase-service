        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontology.mayo.edu/ontologies/clinicalsituationontology/b63f7be3-2ac4-3930-8ab6-09347ff6459b"/>
                    <owl:Restriction>
